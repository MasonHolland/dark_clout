<div class="user-dashboard">
  <p>Name: <%= current_user.name %></p>
  <p>Phone Number: <%= current_user.phone %></p>
  <p>Email: <%= current_user.email %></p>
  <p>Username: <%= current_user.username %></p>

<%= button_to "Add Instagram", "https://api.instagram.com/oauth/authorize/?client_id=#{ENV['INSTAGRAM_ID']}&redirect_uri=http://localhost:3000/&response_type=token&scope=basic+public_content", method: :put, controller: :user_, class: "btn btn-default" %>
<%= button_to "Edit Profile", users_dashboard_edit_path(current_user.username), method: :get, class: "btn btn-default" %>
<%= button_to "Change Password", users_password_reset_path(current_user.username), method: :get, class: "btn btn-default" %>
</div>
